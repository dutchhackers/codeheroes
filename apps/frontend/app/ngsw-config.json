{
  "$schema": "../../../node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/app-icons/*.png", "/favicon.ico", "/index.html", "/manifest.webmanifest", "/*.css", "/*.js", "!/firebase-messaging-sw.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/icons/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif)"
        ]
      }
    },
    {
      "name": "fonts",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/fonts/**/*.(otf|ttf|woff|woff2)"
        ]
      }
    }
  ]
}
