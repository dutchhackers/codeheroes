{
  "name": "database-seeds",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/database-seeds/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/database-seeds",
        "tsConfig": "libs/database-seeds/tsconfig.lib.json",
        "packageJson": "libs/database-seeds/package.json",
        "main": "libs/database-seeds/src/index.ts",
        "assets": ["libs/database-seeds/*.md"]
      }
    },
    "seed": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node dist/libs/database-seeds/src/main.js seed",
        "env": {
          "FIRESTORE_EMULATOR_HOST": "localhost:8080",
          "FIREBASE_AUTH_EMULATOR_HOST": "localhost:9099"
        }
      },
      "configurations": {
        "test": {
          "command": "node dist/libs/database-seeds/src/main.js seed",
          "env": {
            "FIREBASE_PROJECT_ID": "codeheroes-test"
          }
        },
        "production": {
          "command": "node dist/libs/database-seeds/src/main.js seed",
          "env": {
            "FIREBASE_PROJECT_ID": "codeheroes-prod"
          }
        }
      },
      "dependsOn": [
        {
          "target": "build"
        }
      ]
    },
    "reset-progression": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node dist/libs/database-seeds/src/main.js reset-progression",
        "env": {
          "FIRESTORE_EMULATOR_HOST": "localhost:8080"
        }
      },
      "configurations": {
        "test": {
          "command": "node dist/libs/database-seeds/src/main.js reset-progression",
          "env": {
            "FIREBASE_PROJECT_ID": "codeheroes-test"
          }
        }
      },
      "dependsOn": [
        {
          "target": "build"
        }
      ]
    }
  }
}
